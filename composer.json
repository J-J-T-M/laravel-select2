{
    "name": "J-J-T-M/laravel-select2",
    "description": "Um componente Select2 dinâmico e reutilizável para Laravel.",
    "keywords": ["laravel", "select2", "component", "ajax"],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "J-J-T-M",
            "email": "jonatham.junior.18@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "J-J-T-M\\LaravelSelect2\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "J-J-T-M\\LaravelSelect2\\Providers\\Select2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
